var speed= 1
for (var x = 0; x <= 8 ; x += 2) {
    console.log("Mile " + x + " achieved")
    if (((x==0 || x>6) || speed<=5.5)) {
    console.log ("No candy for you Glen Coco")
    } else {
        
        console.log("Give out candy")
    }
}


